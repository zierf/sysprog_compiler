{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Cargo: Run program",
			"type": "cargo",
			"subcommand": "run",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"label": "Cargo: Build program",
			"type": "cargo",
			"subcommand": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"label": "Cargo: Check package and dependencies",
			"type": "cargo",
			"subcommand": "check",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"label": "Cargo: Clean program",
			"type": "cargo",
			"subcommand": "clean",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"label": "Cargo: Lint program",
			"type": "cargo",
			"subcommand": "clippy",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		},
		{
			"label": "Cargo: Test program",
			"type": "cargo",
			"subcommand": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": {
				"kind": "test",
				"isDefault": true
			}
		},
		{
			"label": "Cargo: Generate documentation",
			"type": "cargo",
			"subcommand": "doc",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build"
		}
	]
}
